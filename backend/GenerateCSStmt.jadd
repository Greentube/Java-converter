	// --- IfStmt ---
	
	public void IfStmt.generateCS(com.greentube.javaconverter.CodePrinter out) {
		out.print("if (");
		getCondition().generateCS(out);
		out.print(") ");
		getThen().generateCS(out);
		Stmt e = getElse();
		if (e!=null) {
			out.print("else ");
			e.generateCS(out);
		}
	}  	
	
