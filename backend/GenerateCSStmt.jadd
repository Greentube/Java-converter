	public void ReturnStmt.generateCS(com.greentube.javaconverter.CodePrinter out) {
		Expr r = getResult();
		if (r==null) {
			out.print("return;");
		} else {
			out.print("return ");
			r.generateCS(out);
			out.print(";");
		}
		out.println();		
	}  	
	
