 	// --- Access --- 
 	
  	public void AbstractDot.generateCS(com.greentube.javaconverter.CodePrinter out) {
  		Access a = getRight();
  		if (! (a instanceof ClassInstanceExpr)
  		&&  ! (a instanceof TypeAccess) ) {
  			getLeft().generateCS(out);
  		}
  		a.generateCS(out);
 	}
