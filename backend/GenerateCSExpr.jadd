  	public void AssignSimpleExpr.generateCS(com.greentube.javaconverter.CodePrinter out) {
  		Expr d = getDest();
  		Expr s = getSource();

  		d.generateCS(out);
  		out.print(" = ");

		if (d.type().isChar() && s.type().isInt()) {
			out.print("java.lang.SYSTEM.castToChar(");
  			s.generateCS(out);
  			out.print(")");		
		} else {
  			s.generateCS(out);
		}
  	}

  	// --- Binary ---

  	public void AddExpr.generateCS(com.greentube.javaconverter.CodePrinter out) {
  		// do some case discrimination if this is a string concatenation
  		if (type().isString()) {
  			AddExpr e = this;
  			out.print("System.String.Join(\"\",");
  			params: for (;;) {
 				out.print("java.lang.SYSTEM.str(");
  				e.getLeftOperand().generateCS(out);
  				out.print(")");
				out.print(",");
  				if (e.getRightOperand() instanceof AddExpr) {
  					e = (AddExpr) e.getRightOperand();
  				} else {
 					out.print("java.lang.SYSTEM.str(");
 					e.getRightOperand().generateCS(out);
	  				out.print(")");  	
	  				break params;				
  				}
  			}
  			out.print(")");
  		} else {
	  		getLeftOperand().generateCS(out);
  			out.print("+");
  			getRightOperand().generateCS(out);
  		}
  	}
