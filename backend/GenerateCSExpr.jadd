  	public void ArrayInit.generateCS(com.greentube.javaconverter.CodePrinter out) {
  		out.print("new ");
  		type().generateReferenceCS(out);
  		out.print("{");
  		for (int i=0; i<getNumInit(); i++) {
  			if (i>0) out.print(",");
  			getInit(i).generateCS(out);
  		}
  		out.print("}");
  	}
  	
