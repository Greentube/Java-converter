  	// --- PrimaryExpr ---
  	
  	public void ArrayCreationExpr.generateCS(com.greentube.javaconverter.CodePrinter out) {
  		if (hasArrayInit()) {
  			getArrayInit().generateCS(out);
  		} else {
  			TypeDecl leaf = type();
  			while (leaf.isArrayDecl()) {
  				leaf=leaf.elementType();
  			}
  			out.print("(java.lang.SYSTEM.dim<");
  			leaf.generateReferenceCS(out);
  			out.print(">(");
  			getTypeAccess().generateCS(out);
			out.print("))");
  		}
  	}
  	
  	public void Literal.generateCharacterCS(com.greentube.javaconverter.CodePrinter out, char c) {
  			switch(c) {
  			case '\b': 	out.print("\\b");
  						break;
  			case '\f':  out.print("\\f");	
  			            break;
  			case '\n':  out.print("\\n"); 
  			            break;
  			case '\r':  out.print("\\r");
  			            break;
  			case '\t':  out.print("\\t");
  			            break;
  			case '\\':  out.print("\\\\");
  			            break;
  			case '"':   out.print("\\\"");
  			            break;  			
  			case '\'':  out.print("\\'");
  			            break;  			
  			default: 
  				if (c>=32 && c<=126) {
  				 	out.print(""+c);
  				} else {
  					String hx = Integer.toHexString((int)c);
  					out.print("\\u");
  					for (int j=hx.length(); j<4; j++) {
  						out.print("0");
  					} 
  					out.print(hx);
  				}
  				break;
  			}  	
  	}  	
